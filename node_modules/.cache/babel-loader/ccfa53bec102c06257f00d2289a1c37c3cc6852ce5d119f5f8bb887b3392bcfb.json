import './Profile.css';\r\nimport { useEffect, useState } from 'react';\r\nimport axios from 'axios';\r\n\r\ninterface Excursion {\r\n  id: number;\r\n  title: string;\r\n  description: string;\r\n  imageUrl: string;\r\n  route: string;\r\n}\r\n\r\nconst Profile = () => {\r\n  const [passedExcursions, setPassedExcursions] = useState<Excursion[]>([]);\r\n  const email = localStorage.getItem(\"email\");\r\n\r\n  useEffect(() => {\r\n    if (email) {\r\n      axios.get(`http://localhost:3001/api/passed/${email}`)\r\n        .then(res => setPassedExcursions(res.data))\r\n        .catch(err => console.error(\"Помилка при завантаженні пройдених екскурсій:\", err));\r\n    }\r\n  }, [email]);\r\n\r\n  return (\r\n    <div className=\"profile-container\">\r\n      <div className=\"profile-card\">\r\n        <h1>Вітаємо у вашому кабінеті!</h1>\r\n        <p className=\"intro-text\">\r\n          Дякуємо за реєстрацію у нашому сервісі цифрових екскурсій Вінниччиною.<br />\r\n          Ви стали частиною унікальної подорожі через історію, культуру та сучасність регіону.<br /><br />\r\n\r\n          🔹 Вінниця — місто, що існує понад 650 років та зберігає дух Середньовіччя.<br />\r\n          🔹 Тут розташований найстаріший у Східній Європі водонапірний майдан — Вежа Артинова.<br />\r\n          🔹 У Вінниці працює найвідоміший в Україні світломузичний фонтан “ROSHEN”.<br />\r\n          🔹 Саме тут зберігаються документи часів УНР — української незалежності 1917–1921 рр.<br />\r\n          🔹 Поділля — край, багатий на старовинні замки, підземелля та мальовничі ландшафти.<br />\r\n          🔹 В області понад 100 туристичних об'єктів, доступних для дослідження у цифровому форматі.<br />\r\n          🔹 У Вінницькому краєзнавчому музеї зберігається мамонтова кістка віком понад 30 000 років.<br />\r\n          🔹 Тут можна зустріти архітектурні пам’ятки польської, єврейської, української спадщини.<br /><br />\r\n\r\n          Дослідження екскурсій відкриється після першого проходження маршруту.<br />\r\n          Бажаємо цікавих відкриттів і приємної подорожі!\r\n        </p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Profile;\r\n"],"mappings":";;AAAA,OAAO,eAAe;AACtB,SAASA,SAAS,EAAEC,QAAQ,QAAQ,OAAO;AAC3C,OAAOC,KAAK,MAAM,OAAO;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAU1B,MAAMC,OAAO,GAAGA,CAAA,KAAM;EAAAC,EAAA;EACpB,MAAM,CAACC,gBAAgB,EAAEC,mBAAmB,CAAC,GAAGP,QAAQ,CAAc,EAAE,CAAC;EACzE,MAAMQ,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;EAE3CX,SAAS,CAAC,MAAM;IACd,IAAIS,KAAK,EAAE;MACTP,KAAK,CAACU,GAAG,CAAC,oCAAoCH,KAAK,EAAE,CAAC,CACnDI,IAAI,CAACC,GAAG,IAAIN,mBAAmB,CAACM,GAAG,CAACC,IAAI,CAAC,CAAC,CAC1CC,KAAK,CAACC,GAAG,IAAIC,OAAO,CAACC,KAAK,CAAC,+CAA+C,EAAEF,GAAG,CAAC,CAAC;IACtF;EACF,CAAC,EAAE,CAACR,KAAK,CAAC,CAAC;EAEX,oBACEL,OAAA;IAAKgB,SAAS,EAAC,mBAAmB;IAAAC,QAAA,eAChCjB,OAAA;MAAKgB,SAAS,EAAC,cAAc;MAAAC,QAAA,gBAC3BjB,OAAA;QAAAiB,QAAA,EAAI;MAA0B;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eACnCrB,OAAA;QAAGgB,SAAS,EAAC,YAAY;QAAAC,QAAA,GAAC,yXAC8C,eAAAjB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,gcACQ,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,iXAErB,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,scACI,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,gXACjB,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,8ZACK,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,gbACR,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,4cACE,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,4cACN,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,wdACT,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,+XAE/B,eAAArB,OAAA;UAAAkB,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAK,CAAC,gQAE7E;MA